{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch to Edge",
      "port": 9222,
      "request": "launch",
      "type": "msedge",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [
        "${workspaceFolder}/<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ]
    },
    {
      "name": "Launch to Chrome",
      "port": 9222,
      "request": "launch",
      "type": "chrome",
      "webRoot": "${workspaceFolder}",
      "skipFiles": [
        "${workspaceFolder}/<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ]
    }
  ]
}
